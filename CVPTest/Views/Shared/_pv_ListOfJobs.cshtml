<table class="table table-condensed">

    @foreach (var c in Model.Jobs)
    {
        <tr>
            <td>@c.Id</td>
            <td><img src="@c.PhysicalPath" height="64" /></td>
            <td>@c.LogicalName</td>
            <td>@c.PhysicalName</td>
            <td>
                <button class="btn btn-xs btn-danger"
                        onclick="d(@c.Id)">
                    <span class="fa fa-trash"></span>
                </button>
            </td>
        </tr>
    }
</table>